{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["openapi.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/fanout-pesquisa",
      "dest": "index.py"
    },
    {
      "src": "/openapi.json",
      "dest": "index.py"
    }
  ]
}
